<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:arta='urn:import:kz.arta.synergy.components.client'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with type="kz.arta.synergy.components.client.resources.ImageResources" field="images"/>
    <arta:ArtaFlowPanel>
        <g:Image ui:field="indicator" resource="{images.nodeClosed}" />
        <g:Image ui:field="icon" resource="{images.folder}" />
        <g:InlineLabel ui:field="label" />
        <g:FlowPanel ui:field="content"/>
    </arta:ArtaFlowPanel>
</ui:UiBinder>