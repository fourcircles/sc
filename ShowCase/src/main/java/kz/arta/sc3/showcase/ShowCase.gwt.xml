<?xml version="1.0" encoding="UTF-8"?>
<module>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="kz.arta.synergy.components.SynergyComponents"/>
    <inherits name="kz.arta.synergy.components.style.ComponentStyle"/>
    <inherits name="kz.arta.i18n.I18N"/>

    <entry-point class='kz.arta.sc3.showcase.client.ShowCase'/>
    <set-configuration-property name="CssResource.style" value="pretty"/>

    <add-linker name="xsiframe"/>

    <extend-property name="locale" values="ru"/>
    <extend-property name="locale" values="kk"/>
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="ar"/>
    <extend-property name="locale" values="ky"/>

    <set-property-fallback name="locale" value="ru"/>

    <collapse-all-properties />

    <set-property name="user.agent" value="ie9,gecko1_8,ie9,safari" />

</module>